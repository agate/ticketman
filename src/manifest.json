{
  "manifest_version": 2,

  "name": "Ticketman",
  "description": "A chrome extension for managing github issues",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
